voidCamp_Due_3W(Int campID) {
campRecord = zoho.crm.getRecordById("Camps",campID);
addDays = campRecord.get("Number_of_Days");
info addDays;
startDate = campRecord.get("Camp_Start_Date");
info startDate;
addDate = startDate.addDay(addDays);
info addDate;
mapDate = Map();
mapDate.put("Google_Calendar_End_Date",addDate);
endGoogleDate = zoho.crm.updateRecord("Camps",campID,mapDate);
info endGoogleDate;
addDays = addDays - 1;
startDate = campRecord.get("Camp_Start_Date");
info startDate;
addDate = startDate.addDay(addDays);
info addDate;
mapDate = Map();
mapDate.put("Camp_End_Date",addDate);
endDate = zoho.crm.updateRecord("Camps",campID,mapDate);
info endDate;
}
